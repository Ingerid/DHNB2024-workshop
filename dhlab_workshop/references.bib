@thesis{olsen_subjektplassering_2019,
	title = {Subjektplassering i moderne norsk - en korpusbasert analyse av leddstillingsvariasjon i bokmål det siste hundreåret},
	url = {https://www.duo.uio.no/handle/10852/69263},
	abstract = {Det er mulig å identifisere tre ulike subjektposisjoner i norsk: i forfeltet, i midtfeltet før negasjon og i midtfeltet etter negasjon. Det er observert ulike mønstre i subjektfordelinga i de ulike skandinaviske språka (Østbø Munch 2013). I denne oppgava har jeg undersøkt åssen denne leddstillingsvariasjonen arter seg i moderne bokmål. Formålet har vært å kartlegge åssen subjektet fordeler seg over de mulige posisjonene, å undersøke om det har skjedd en endring siden første halvdel av nittenhundretallet, samt å undersøke hvilke formelle og informasjonsstrukturelle faktorer som påvirker denne plasseringa. Tidligere forskning har pekt på at formelle faktorer som setningstype og forma på subjektet påvirker plasseringa i skandinaviske språk ((Østbø Munch 2013), (Andréasson og Larsson 2017)). I tillegg har flere antatt at informasjonsstruktur spiller en rolle for subjektplassering ((Westergaard 2011) og (Svenonius 2002)). For eldre bokmål benyttet jeg meg av Nasjonalbibliotekets {API} for analyse av fulltekst, og satte sammen et korpus bestående av 1000 verk fra Nasjonalbibliotekets boksamling, fra perioden 1910-1925. For nyere bokmål benyttet jeg meg av Leksikografisk bokmålskorpus, som inneholder verk fra 1985-2013. Fra begge korpus har jeg gjennomgått 1000 setninger som inneholder "ikke". Disse har blitt kategorisert etter setningstype, form på subjektet og subjektplassering. Korpusstudien viser at forfeltet er den klart vanligste posisjonen for subjektet i helsetninger. I helsetninger står pronominelle subjekt nesten aldri etter negasjon, mens ikke-pronominelle subjekt fordeler seg temmelig jamt før og etter negasjon i midtfeltet. I materialet fra Nasjonalbiblioteket står ikke-pronominelle subjekt også signifikant oftere i forfeltet enn pronominelle subjekt. Denne forskjellen er imidlertid utjamna i materialet fra Leksikografisk bokmålskorpus. Utover dette finner jeg ingen større forskjeller mellom det eldre og det yngre materialet. Informasjonsstruktur viser seg særlig å ha betydning for subjektplasseringa i helsetninger i det eldre materialet. Her virker det som subjektet i større grad leses som tematisk desto tidligere i setninga det står. Informasjonsstruktur kan i langt mindre grad forklare subjektplasseringa i det nyere materialet. Forfeltet virker til en viss grad å ha en tematiserende funksjon, men jeg finner også svært mange tematiserte subjekt i midtfeltet, både før og etter negasjon. Uavhengig av tidsperiode virker informasjonsstruktur og setningas faktivitet å påvirke subjektplassering i leddsetninger.},
	type = {Master thesis},
	author = {Olsen, Ida Keihl},
	urldate = {2023-01-19},
	date = {2019},
	note = {Accepted: 2019-08-21T23:45:57Z},
}

@article{hemstad_scandinavianism_2018,
	title = {Scandinavianism},
	volume = {13},
	issn = {1807-9326, 1874-656X},
	url = {http://berghahnjournals.com/view/journals/contributions/13/1/choc130102.xml},
	doi = {10.3167/choc.2018.130102},
	abstract = {In the historiography describing the pan-Scandinavian movement that
started gaining ground in the late 1830s, Scandinavianism has been widely
employed as a historical category, usually without any discussion regarding
the actual emergence of the concept itself. This article discusses when
and why Scandinavianism entered into political language as a powerful
new concept capable of setting a future-oriented agenda. After analyzing
digitized newspaper material and other relevant publications in Norway,
Sweden, and Denmark and after using a combination of distant and close
reading, it concludes that Scandinavianism as a neologism only appeared
as late as 1843. This article aims to contribute to a conceptual transnational
history of Scandinavianism by examining its emergence, reception, and
discursive context in the early 1840s.},
	pages = {1--21},
	number = {1},
	journaltitle = {Contributions to the History of Concepts},
	author = {Hemstad, Ruth},
	urldate = {2023-01-19},
	date = {2018-06-01},
}

